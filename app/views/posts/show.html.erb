<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Body:</strong>
  <%= @post.body %>
</p>

<p>
  <strong>Account:</strong>
  <%= @post.account_id %>
</p>

<p>
  <strong>Permalink:</strong>
  <%= @post.permalink %>
</p>


<% @post.gallery.photos.limit(4).each do |photo| %>
  <img src="<%= photo.image(:medium_s) %>" width="24%">
<% end %>
<br>
