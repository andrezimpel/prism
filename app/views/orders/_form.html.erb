<%= simple_form_for(@order) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :account_id %>
    <%= f.input :client_id %>
    <%= f.input :state %>
    <%= f.input :amount %>
    <%= f.input :address_id %>
    <%= f.input :shopping_cart_id %>
    <%= f.input :ip_address %>
    <%= f.input :card_expires_on %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
