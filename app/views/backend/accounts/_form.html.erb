<%= simple_form_for(@account) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :subdomain %>
    <%= f.input :user_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Save" %>
    <%= link_to 'Cancel', prism_root_path, class: "btn btn-danger btn-link" %>
  </div>
<% end %>
